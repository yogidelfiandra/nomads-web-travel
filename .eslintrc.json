{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "import/no-relative-packages": 0,
    "linebreak-style": 0,
    "import/no-extraneous-dependencies": 0,
    "no-tabs": 0,
    "no-underscore-dangle": 0,
    "operator-linebreak": 0
  }
}
